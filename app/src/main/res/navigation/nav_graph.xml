<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/userFragment">

    <fragment
        android:id="@+id/userFragment"
        android:name="com.loveworldapps.fairmoneytest.ui.user.UserFragment"
        android:label="UserFragment" >
        <action
            android:id="@+id/action_userFragment_to_userDetailFragment"
            app:destination="@id/userDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/userDetailFragment"
        android:name="com.loveworldapps.fairmoneytest.ui.userDetail.UserDetailFragment"
        android:label="UserDetailFragment" >
        <argument
            android:name="user"
            app:argType="com.loveworldapps.fairmoneytest.api.models.User" />
    </fragment>
</navigation>